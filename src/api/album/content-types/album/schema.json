{
  "kind": "collectionType",
  "collectionName": "albums",
  "info": {
    "singularName": "album",
    "pluralName": "albums",
    "displayName": "Album",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "private": true,
      "unique": true
    },
    "release_date": {
      "type": "date"
    },
    "fanlink": {
      "type": "string"
    },
    "cover_art": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "songs": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::song.song",
      "mappedBy": "albums"
    },
    "title": {
      "type": "string",
      "required": true
    },
    "primary_artist": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::artist.artist"
    }
  }
}
